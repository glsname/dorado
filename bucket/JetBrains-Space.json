    },
    "shortcuts": [
        [
            "JetBrains Space.exe",
            "JetBrains Space"
        ]
    ],
    "checkver": {
        "url": "https://download-cf.jetbrains.com/space/latest.yml",
        "regex": "version:\\s*([\\d.]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://download.jetbrains.com/space/jetbrains-space-$version.exe#/cosi.7z",
                "hash": {
                    "url": "$baseurl/latest.yml",
                    "regex": "sha512:\\s+$base64"
                }
            }
        }
    }
}
