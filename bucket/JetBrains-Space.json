{
    "version": "2021.4.2",
    "description": "Desktop Application for JetBrains Space - integrated team environment",
    "homepage": "https://download-cf.jetbrains.com/space/jetbrains-space.exe",
    "license": "Freeware",
    "architecture": {
        "64bit": {
            "url": "https://download.jetbrains.com/space/jetbrains-space-2021.4.2.exe#/cosi.7z",
            "hash": "sha512:f58d88cd42b56a3663f96621cb68ceb120843906a01f983d8bf8918b26d14215560941ea4c846aa9ce3ab534a0170544a1a5b4d86d15f05a0c76e059f1cd1af2",
            "pre_install": [
                "try { Get-Command 'Test-Persistence' -ErrorAction 'Stop' | Out-Null } catch { error 'Shovel flavour of scoop is required'; exit 258 }",
                "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" $dir",
                "Remove-AppDirItem '$*', 'Uninstall*'"
