{
    "version": "2023.1.2",
    "description": "Desktop Application for JetBrains Space - integrated team environment",
    "homepage": "https://download-cf.jetbrains.com/space/jetbrains-space.exe",
    "license": "Freeware",
    "architecture": {
        "64bit": {
            "url": "https://download.jetbrains.com/space/jetbrains-space-2023.1.2.exe#/cosi.7z",
            "hash": "sha512:992fb836c804a3462910a277a79f5b976a0848b5bb4397ac9be53dc83def008e9278f4996a2682e377fe43e50f510e3e7be1c9f78fcab39e067ca36d12f03dcf",
            "pre_install": [
                "try { Get-Command 'Test-Persistence' -ErrorAction 'Stop' | Out-Null } catch { error 'Shovel flavour of scoop is required'; exit 258 }",
