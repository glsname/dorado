{
    "version": "2023.1.0",
    "description": "Desktop Application for JetBrains Space - integrated team environment",
    "homepage": "https://download-cf.jetbrains.com/space/jetbrains-space.exe",
    "license": "Freeware",
    "architecture": {
        "64bit": {
            "url": "https://download.jetbrains.com/space/jetbrains-space-2023.1.0.exe#/cosi.7z",
            "hash": "sha512:69f19ad75e461c7cc5fbcf379b5349c7386463e900cad29ca764b477f1ea9274fc314e29fb7629e335f55929db1db5d56c7af609596c39c5a1caa091b877aee1",
            "pre_install": [
                "try { Get-Command 'Test-Persistence' -ErrorAction 'Stop' | Out-Null } catch { error 'Shovel flavour of scoop is required'; exit 258 }",
