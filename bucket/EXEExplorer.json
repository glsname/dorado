    "installer": {
        "script": [
            "$p = appdir 'TaskManagerDeLuxe' $global",
            "if (Test-Path \"$p\\current\") { Copy-Item \"$dir\\EXE.exe\" \"$p\\current\" -Force }"
        ]
    },
    "architecture": {
        "64bit": {
            "shortcuts": [
                [
                    "EXE64.exe",
                    "MiTeC\\EXE Explorer"
                ]
            ]
        },
        "32bit": {
            "shortcuts": [
                [
                    "EXE.exe",
                    "MiTeC\\EXE Explorer"
                ]
            ]
        }
    },
    "checkver": {
        "url": "https://www.mitec.cz/webupdate.html",
        "regex": "EXEVersion\"\\s*CONTENT=\"([\\d.]+)\""
    },
    "autoupdate": {
        "url": "https://www.mitec.cz/Downloads/EXE.zip"
    }
}
